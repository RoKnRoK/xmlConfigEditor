<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.0//EN"
        "http://google-web-toolkit.googlecode.com/svn/releases/2.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="XmlConfigEditorGwt">

    <!-- Inherit the core Web Toolkit stuff.                  -->
    <inherits name='com.google.gwt.user.User'/>
    <inherits name="com.rok.xml.XmlConfigEditorModel"/>

    <!-- Specify the app entry point class.                   -->
    <entry-point class='com.rok.gwt.client.XmlConfigEditorGwt'/>

    <set-property name="user.agent" value="safari" />

    <!-- Specify the app servlets.                   -->
    <servlet path='/XmlConfigEditorGwtService'
             class='com.rok.gwt.server.XmlConfigEditorGwtServiceImpl'/>

    <source path="client"/>
    <source path="config_dto"/>
    <source path="settings"/>
</module>